{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "strictNullChecks": true,
    "types": ["astro/client"],
    "paths": {
      "@assets/*": ["./public/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@components/*": ["./src/components/*"],
      "@models/*": ["./src/models/*"]
    }
  }
}
